ck\"\n                                @show=\"getActivePeople(scope.row.id)\"\n                                :width=\"550\"\n                                placement=\"left\"\n                            >\n                                <el-table :data=\"activeLogs\" border>\n                                    <el-table-column\n                                        align=\"center\"\n                                        type=\"index\"\n                                    ></el-table-column>\n                                    <el-table-column\n                                        align=\"center\"\n                                        prop=\"userName\"\n                                        label=\"参与人员\"\n                                    ></el-table-column>\n                                    <el-table-column\n                                        align=\"center\"\n                                        prop=\"userPhone\"\n                                        label=\"联系电话\"\n                                    ></el-table-column>\n                                    <el-table-column\n                                        align=\"center\"\n                                        prop=\"createTime\"\n                                        label=\"报名时间\"\n                                    ></el-table-column>\n                                </el-table>\n                                <el-button\n                                    type=\"primary\"\n                                    style=\"font-size: 18px\"\n                                    slot=\"reference\"\n                                    >\n                                    参与人员</el-button\n                                >\n                            </el-popover>\n                        </template>\n                    </el-table-column>\n                    <el-table-column\n                        v-if=\"userType == 0\"\n                        align=\"center\"\n 